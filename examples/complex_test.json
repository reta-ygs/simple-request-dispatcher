{
  "method": "POST",
  "path": "/post",
  "headers": {
    "Content-Type": "application/x-www-form-urlencoded",
    "X-Test-ID": {
      "!uuid": []
    }
  },
  "params": {
    "username": {
      "!var": "test_user"
    },
    "timestamp": {
      "!timestamp": []
    },
    "encoded_payload": {
      "!base64_encode": {
        "!concat": [
          "payload:",
          {"!var": "test_user"},
          ":",
          {"!random": [1000]}
        ]
      }
    }
  },
  "variables": {
    "test_user": "admin"
  }
}